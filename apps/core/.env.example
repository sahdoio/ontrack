# Application
NODE_ENV=development
PORT=3000
APP_NAME=OnTrack

# Database
DB_HOST=localhost
DB_PORT=5432
DB_USERNAME=postgres
DB_PASSWORD=postgres
DB_DATABASE=ontrack
DB_SYNCHRONIZE=false
DB_LOGGING=true

# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your_access_key
AWS_SECRET_ACCESS_KEY=your_secret_key

# S3
S3_BUCKET_NAME=ontrack-csv-uploads

# SNS
SNS_TOPIC_ARN=arn:aws:sns:us-east-1:123456789012:ontrack-notifications

# SQS
SQS_NOTIFICATION_QUEUE_URL=https://sqs.us-east-1.amazonaws.com/123456789012/ontrack-notifications
SQS_CSV_PROCESSING_QUEUE_URL=https://sqs.us-east-1.amazonaws.com/123456789012/ontrack-csv-processing

# Email Service (for notifications)
EMAIL_FROM=noreply@ontrack.com